{
  "name": "mobile-glasschadenmelden",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3002",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "capacitor:init": "npx cap init MobileGlasschadenMelden com.glasschadenmelden.mobile --web-dir out",
    "capacitor:add:ios": "npx cap add ios",
    "capacitor:add:android": "npx cap add android",
    "capacitor:sync": "npx cap sync",
    "capacitor:open:ios": "npx cap open ios",
    "capacitor:open:android": "npx cap open android",
    "mobile:build": "next build && next export && npx cap sync",
    "mobile:ios": "npm run mobile:build && npx cap open ios",
    "mobile:android": "npm run mobile:build && npx cap open android"
  },
  "dependencies": {
    "@capacitor/android": "^5.5.1",
    "@capacitor/app": "^5.0.6",
    "@capacitor/core": "^5.5.1",
    "@capacitor/haptics": "^5.0.6",
    "@capacitor/ios": "^5.5.1",
    "@capacitor/keyboard": "^5.0.6",
    "@capacitor/status-bar": "^5.0.6",
    "@supabase/ssr": "^0.0.10",
    "@supabase/supabase-js": "^2.39.0",
    "lucide-react": "^0.562.0",
    "next": "14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sonner": "^1.3.1"
  },
  "devDependencies": {
    "@capacitor/cli": "^5.5.1",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
